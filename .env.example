# Database Connection
PG_CONNECTION_STRING=postgres://user:password@localhost:5432/clawpad_db

# AI API Keys (from package.json deps like @anthropic-ai/sdk, openai)
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...

# Blockchain RPC URLs (for Solana, Base, BNB via @solana/web3.js, viem, etc.)
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
BASE_RPC_URL=https://mainnet.base.org
BNB_RPC_URL=https://bsc-dataseed.binance.org

# Venue-Specific Keys (if required, e.g., for Pumpfun, Clanker)
PUMPFUN_API_KEY=your_pumpfun_key_here
CLANKER_API_KEY=your_clanker_key_here

# Other Configurations
PORT=3000
MOLTBOOK_API_URL=https://api.moltbook.com
IPFS_GATEWAY=https://ipfs.io/ipfs/
SECRET_KEY=your_random_secret_for_encryption
